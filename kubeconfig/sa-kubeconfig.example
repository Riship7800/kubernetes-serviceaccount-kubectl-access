apiVersion: v1
kind: Config
clusters:
- name: sa-cluster
  cluster:
    server: https://<API_SERVER>
    certificate-authority-data: <BASE64_CA>
users:
- name: my-sa
  user:
    token: <SERVICE_ACCOUNT_TOKEN>
contexts:
- name: sa-context
  context:
    cluster: sa-cluster
    user: my-sa
current-context: sa-context
